  <script>
    function SubData() {
      let CatId = document.getElementById('Cat_id').value;
      $.ajax({
        url: "/subcategory/getSubCatData",
        method: "post",
        data: {
          categoryId: CatId
        },
        success: function (res) {
          // console.log(res);
          $("#SubCat_id").html(res);
        }
      })
    }
  
    function exCatData() {
      let SubId = $('#SubCat_id').val();
      $.ajax({
        url: "/excategory/getexCatData",
        method: "post",
        data: {
          subCatId: SubId
        },
        success: function (res) {
          // console.log(res);
          $("#ExCat_id").html(res);
        }
      })
    }
  
    function btData() {
      let exId = $("#ExCat_id").val();
      let catId = $("#Cat_id").val();
      let subId = $("#SubCat_id").val();
      $.ajax({
        url: "/product/getBtData",
        method: "post",
        data: {
          ExCat_id: exId,
          Cat_id: catId,
          SubCat_id: subId
        },
        success: function (res) {
          $("#btData").html(res);
        }
      })
  
    }
  </script>
  
  
